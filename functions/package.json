{
    "name": "functions",
    "version": "1.0.0",
    "description": "Student Registration App Cloud Functions",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "22"
    },
    "dependencies": {
        "firebase-admin": "^13.0.2",
        "firebase-functions": "^6.3.0",
        "express": "^4.21.2",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "typescript": "^5.7.2",
        "@types/express": "^5.0.0",
        "@types/cors": "^2.8.17",
        "@types/node": "^22.12.0"
    },
    "private": true
}